{
   "type": "sqlite",
   "synchronize": false,
   "database": "/root/db/database.sqlite",
   "logging": true,
   "entities": [
      "src/entity/**/*.ts",
      "src/entity/**/*.js"
   ],
   "migrations": [
      "src/migration/**/*.ts"
   ],
   "subscribers": [
      "src/subscriber/**/*.ts",
      "src/subscriber/**/*.js"
   ],
   "cli": {
      "entitiesDir": "src/entity",
      "migrationsDir": "src/migration",
      "subscribersDir": "src/subscriber"
   }
}
